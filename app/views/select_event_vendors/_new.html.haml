- @select_event_vendor = SelectEventVendor.new(:select_event_id => @select_event.id)
- vendors = select_event_vendor_list( @select_event )
.modal-wrapper
  .modal-header

    %button.close{:type=>"button","data-dismiss"=>"modal"}
      &times;
    %br
  .modal-body

    = form_for [@select_event, @select_event_vendor] do |f|
      = render :partial => "select_event_vendors/properties", :locals => {select_event: @select_event, f: f, select_event_vendor: @select_event_vendor, vendors: vendors }
        
      .modal-footer

        = f.submit "Add Vendor", :class => "btn btn-primary"